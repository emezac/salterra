<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity ActivityMoves.
    -->
    <changeSet id="20221117205242-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="activity_moves_id"
                                 baseTableName="rel_activity_moves__choices"
                                 constraintName="fk_rel_activity_moves__choices__activity_moves_id"
